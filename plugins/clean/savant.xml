<project>
  <target name="jar">
    <mkdir dir="target/jars"/>
    <jar jarfile="target/jars/${project.name}-${project.version}.jar">
      <fileset dir="src/groovy/main" includes="**/*"/>
    </jar>
  </target>

  <target name="test" depends="jar">
  </target>

  <target name="int" depends="test">
    <publish integrationbuild="true"/>
  </target>
</project>
